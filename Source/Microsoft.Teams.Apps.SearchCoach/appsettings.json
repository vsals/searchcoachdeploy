{
  "AzureAd": {
    "TenantId": "{{ Application tenant id }}",
    "Instance": "https://login.microsoftonline.com/",
    "ApplicationIdURI": "{{ Application Id URI }}",
    "ValidIssuers": "https://login.microsoftonline.com/TENANT_ID/v2.0,https://sts.windows.net/TENANT_ID/",
    "GraphScope": "openid profile offline_access Group.Read.All User.Read.All"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "ApplicationInsights": {
    "InstrumentationKey": "{{ Telemetry instrumentation key }}",
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information"
    }
  },
  "App": {
    "AppBaseUri": "{{ Application base path }}",
    "TenantId": "{{ Application tenant id }}",
    "ManifestId": "{{ Application end user manifest id }}",
    "CardCacheDurationInHour": 1,
    "CacheDurationInMinutes": 60
  },
  "Storage": {
    "ConnectionString": "{{ Storage Table connection string }}"
  },
  "i18n": {
    "DefaultCulture": "en-US",
    "SupportedCultures": "en-US"
  },
  "MicrosoftAppId": "{{ Bot Id }}",
  "MicrosoftAppPassword": "{{ Bot Password }}",
  "UseKeyVault": true,
  "KeyVaultUrl": {
    "BaseURL": "[Azure KeyVault base URL]"
  },
  "BingSearch": {
    "ApiUrl": "{{ Bing search API URL. }}",
    "ApiKey": "{{ Bing search API Key }}",
    "SafeSearch": "{{ Bing search safe search type }}",
    "DefaultCountryCode": "{{ Default country code for Bing search API }}"
  }
}