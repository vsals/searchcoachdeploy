{
	"defaultSeverity": "error",
	"extends": [
		"tslint-microsoft-contrib/recommended"
	],
	"linterOptions": {
		"exclude": [
			"node_modules/**/*.ts"
		]
	},
	"rules": {
		"jsx-no-lambda": false,
		"member-access": false,
		"no-console": false,
		"ordered-imports": false,
		"quotemark": false,
		"semicolon": false,
		"match-default-export-name": false
	},
	"rulesDirectory": [
	]
}